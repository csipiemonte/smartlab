          "cached_js_script_hash": "e2a5deaedd8481b8024dc0987231141e", 
          "language": "BASIC", 
          "onshow": "frmTrack_onshow()", 
          "script": "Dim nmsg\nDim timerRefSend\nDim intervalSend\nDim maxmsg\nDim req\nDim data\nDim increment\nDim stepRoute\nDim isInPause\n\n' initial settings\nFunction frmTrack_onshow()\n  cmbRoute.addItem(\"Torino - via Filadelfia\", \"torino1.xml\")\n  cmbRoute.addItem(\"Torino - corso Agnelli - Beinasco\", \"torino2.xml\")\n  cmbRoute.addItem(\"Torino - tangenziale\", \"torino3.xml\")\n  cmbRoute.addItem(\"Torino - corso grosseto - corso agnelli\", \"torino4.xml\")\n  \n  intervalSend = 1000\n  stepRoute = 1\n  \n  cmbRoute_onchange()\n  nmsg = 0\n  increment = stepRoute\n  isInPause = False\n  \nEnd Function\n\n' manage the play/stop button\nFunction btnPlay_onclick()\n  If btnPlay.text = \"Play\" Then\n    'Start sending data\n    nmsg = 0\n    timerRefSend = SetInterval (onTimerSend, intervalSend)\n    btnPlay.text = \"Pause\"\n    sldSend.disabled = True\n    cmbRoute.disabled = True\n    sldStep.disabled = True\n    ftLoop.disabled = True\n    btnStopSend.disabled = False\n    isInPause = False\n  ElseIf btnPlay.text = \"Resume\" Then \n    btnPlay.text = \"Pause\"\n    isInPause = False\n  Else  \n    btnPlay.text = \"Resume\"\n    isInPause = True\n  End If \nEnd Function\n\nFunction btnStopSend_onclick()\n   'Stop data\n    ClearInterval(timerRefSend)\n    isInPause = False\n    sldSend.disabled = False\n    cmbRoute.disabled = False\n    ftLoop.disabled = False\n    sldStep.disabled = False\n    btnStopSend.disabled = True\n    btnPlay.text = \"Play\"\nEnd Function\n\n' manage the \"interval send\" timer slider\nFunction sldSend_onchange()\n  intervalSend = sldSend.value\nEnd Function\n\n' manage the \"step\" slider\nFunction sldStep_onchange()\n  stepRoute = Int(sldStep.value)\n  increment = stepRoute\nEnd Function\n\n\n' send data to platform\nFunction onTimerSend()\n  Dim jt\n    If isInPause = False Then \n      lblSendN.Caption = \"Sent \" & (nmsg + 1) & \" / \" &  maxmsg & \" GPS points\"\n      \n      nmsg = nmsg + increment\n      \n      If nmsg >= maxmsg Or nmsg < 0 Then\n        If ftLoop.value() = \"On\" Then\n            increment = - increment\n            Exit Sub \n        Else\n          btnPlay_onclick()\n        End If\n      \n      Else\n        jt = \"{\"\"stream\"\": \"\"\" + NomeStream + \"\"\", \"\n        jt = jt + \"\"\"sensor\"\": \"\"\" + SensorID + \"\"\", \"\n        jt = jt + \"\"\"values\"\": [{\"\n        jt = jt + \"\"\"time\"\": \"\"\" +  TimeStamp() + \"\"\", \"\n        jt = jt + \"\"\"components\"\": {\"\n        jt = jt + \"\"\"longitude\"\": \"\"\" + data.trkpt[nmsg].lon + \"\"\", \"\n        jt = jt + \"\"\"latitude\"\": \"\"\" + data.trkpt[nmsg].lat + \"\"\", \"\n        jt = jt + \"\"\"altitude\"\": \"\"\" + data.trkpt[nmsg].ele + \"\"\", \"\n        jt = jt + \"\"\"speed\"\": 0.0}}]} \"\n        \n        lblLat2.Caption = data.trkpt[nmsg].lat\n        lblLon2.Caption = data.trkpt[nmsg].lon\n\n        JavaScript\n          var xmlHttp = new XMLHttpRequest();\n          xmlHttp.open( \"POST\", urlSend, True);\n          xmlHttp.setRequestHeader(\"Authorization\", \"Basic c21hcnRsYWI6VXgwbXU1aXQ=\"); \n          xmlHttp.setRequestHeader(\"Content-Type\", \"application/json \"); \n          xmlHttp.send( jt );\n        End JavaScript\n      End If\n    End If \nEnd Function\n\n' manage the \"route selection\" combo box and load the route data\nFunction cmbRoute_onchange()\n   req = Ajax(urlRoute & cmbRoute.selectedValue())\n   If !req Then Exit Function\n  If req.readyState <> 4 Then Exit Function 'ignore progress reports\n  \n  If req.status <> 200 Then 'failure\n    MsgBox(\"Error: \" & req.err)\n    Exit Function\n  End If\n\n  If Left(req.responseText,5)<>\"<?xml\" Then\n    MsgBox(\"XML not received\" & req.responseText)\n    Exit Function\n  End If\n  data = $.xml2json(req.responseText)\n  maxmsg = data.trkpt.length\n  lblPoint.Caption = maxmsg\nEnd Function\n\n\n'manage the headerBar button\nFunction hdrTrack_onclick(button)\n  If TypeName(button)=\"object\" Then Exit Function\n  \n  ' manage the \"save\" button click\n  If button = \"Home\" Then \n      If btnPlay.text = \"Play\" Then\n          ChangeForm(frmHome)\n      Else\n          btnPlay_onclick()\n          ChangeForm(frmHome)\n      End If\n  End If\nEnd Function\n\n\n\n", 
